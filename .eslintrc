{
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "eqeqeq": 2,
        "object-curly-spacing": 0,
        "prefer-template": "error",
        "wrap-iife": [
            2,
            "any"
        ],
        "no-undef": 2,
        "no-mixed-spaces-and-tabs": 2,
        "indent": [
            2,
            4,
            {
                "SwitchCase": 1
            }
        ],
        "max-len": [
            2,
            120
        ],
        "quotes": [
            2,
            "double"
        ],
        "consistent-this": [
            2,
            "self"
        ],
        "key-spacing": [
            2,
            {
                "afterColon": true
            }
        ],
        "space-before-blocks": [
            2,
            "always"
        ],
        "space-infix-ops": 2,
        "curly": [
            2,
            "all"
        ],
        "operator-linebreak": [
            2,
            "before"
        ],
        "no-multi-str": 2,
        "no-trailing-spaces": 2,
        "space-unary-ops": [
            2,
            {
                "nonwords": false,
                "overrides": {}
            }
        ],
        "one-var": [
            2,
            "never"
        ],
        "keyword-spacing": [
            2,
            {}
        ],
        "eol-last": 2,
        "array-bracket-spacing": [
            2,
            "never"
        ],
        "space-in-parens": [
            2,
            "never"
        ],
        "no-multiple-empty-lines": 2,
        "no-var": "error",
		"object-shorthand": ["error", "always"],
		"prefer-arrow-callback": "error",
		"prefer-const": ["error", {
			"destructuring": "all"
		}],
		"prefer-numeric-literals": 2
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    }
}
